class Solution {
public:
    vector<vector<int>> transpose(vector<vector<int>>& mat) {
        int n=mat.size(),m=mat[0].size();
        vector<vector<int>> v(m,vector<in867. >(n,0));
        for(int i=0;i<m;i++){
            for(int j=0;j<n;j++) v[i][j]=mat[j][i];
        }
        return v;
    }
};
